    { "src": "^/terms$", "dest": "/terms.html" },
    { "src": "^/ops$", "dest": "/ops.html" },

    { "src": "^/reports/(.*)$", "dest": "https://onebc34tesijmjtq.public.blob.vercel-storage.com/reports/$1" },

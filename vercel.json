{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/vin$", "dest": "/vin.html" },
    { "src": "^/about$", "dest": "/about.html" },
    { "src": "^/contact$", "dest": "/contact.html" },
    { "src": "^/privacy$", "dest": "/privacy.html" },
    { "src": "/.*", "dest": "/index.html", "headers": { "Cache-Control": "no-cache" } }
  ]
}